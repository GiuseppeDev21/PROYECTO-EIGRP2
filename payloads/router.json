{
  "Cisco-IOS-XE-native:native": {
    "router": {
      "Cisco-IOS-XE-eigrp:router-eigrp": {
        "eigrp": {
          "classic-mode": [
            {
              "autonomous-system": 100,
              "network": {
                "address-wildcard": [
                  {
                    "ipv4-address": "192.168.10.0",
                    "wildcard": "0.0.0.255"
                  },
                  {
                    "ipv4-address": "192.168.20.0",
                    "wildcard": "0.0.0.255"
                  }
                ]
              }
            }
          ],
          "named-mode": [
            {
              "name": "ZONA1",
              "address-family": {
                "ipv6": {
                  "af-ipv6": [
                    {
                      "unicast": "unicast",
                      "autonomous-system": 100,
                      "topology": {
                        "topo-base": [
                            {
                              "topology-base": "base"
                            }
                        ]
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      }
    }
  }
}